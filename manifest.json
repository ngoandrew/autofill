{
  "name": "AutoFill Forms",
  "short_name": "iafforms",
  "description": "One click form filler that supports multiple profiles",
  "author": "AVN",
  "version": "1.0",
  "manifest_version": 2,
  "permissions": [
    "storage",
    "tabs",
    "notifications",
    "clipboardWrite",
    "contextMenus",
    "<all_urls>",
    "downloads"
  ],
  "browser_action": {
    "default_icon": {
      "19": "data/icons/19.png",
      "38": "data/icons/38.png"
    },
    "default_popup": "data/popup/index.html"
  },
  "background": {
    "persistent": false,
    "scripts": [
      "lib/defaults.js",
      "lib/background.js"
    ]
  },
  "icons": {
    "16": "data/icons/16.png",
    "48": "data/icons/48.png",
    "128": "data/icons/128.png"
  },
  "commands": {},
  "options_ui": {
    "page": "data/options/index.html",
    "chrome_style": true
  }
}
